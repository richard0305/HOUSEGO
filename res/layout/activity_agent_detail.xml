<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dumu.housego.AgentDetailActivity"
    tools:ignore="HardcodedText,RtlHardcoded,ContentDescription,UselessParent" >

    <RelativeLayout
        android:id="@+id/rl_new_house_title"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#e12442" >

        <LinearLayout
            android:id="@+id/ll_agentdetail_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:src="@drawable/white_left" />
        </LinearLayout>

        <TextView
            android:id="@+id/tv_new_house_detail_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="经纪人详情"
            android:textColor="#ffffff" />
    </RelativeLayout>

    <com.dumu.housego.util.MyReboundScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/rl_new_house_title" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >
            <!-- 框內內容，scrollView -->
            
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <com.dumu.housego.util.CircleImageView
                    android:id="@+id/circleImageView1"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:src="@drawable/agent1" />

                <TextView
                    android:id="@+id/tv_agent_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/circleImageView1"
                    android:layout_toRightOf="@+id/circleImageView1"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="5dp"
                    android:text="胡歌店经理" />

                <TextView
                    android:id="@+id/tv_agent_year"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/tv_agent_name"
                    android:layout_marginTop="5dp"
                    android:layout_below="@+id/tv_agent_name"
                    android:textSize="12sp"
                    android:text="2到3年" />

                <TextView
                    android:id="@+id/tv_agent_mendian"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/tv_agent_year"
                    android:layout_marginTop="5dp"
                    android:layout_below="@+id/tv_agent_year"
                    android:text="龙华区创意门店"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_agent_biaoqian"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/tv_agent_mendian"
                    android:layout_marginTop="5dp"
                    android:layout_below="@+id/tv_agent_mendian"
                    android:text="销售达人"
                    android:textColor="#ff0000"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_agent_haopinglv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/tv_agent_year"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dp"
                    android:text="78%"
                    android:textColor="#ff0000"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/TextView01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/tv_agent_year"
                    android:layout_toLeftOf="@+id/tv_agent_haopinglv"
                    android:text="好评率:"
                    android:textColor="#ff0000"
                    android:textSize="12sp" />

            </RelativeLayout>
            
            <Button 
               android:layout_width="match_parent"
               android:background="#c9c9cb"
               android:layout_height="1dp"
               />
            <LinearLayout 
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <RelativeLayout 
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <TextView 
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="30"
                        android:layout_marginTop="5dp"
                        android:id="@+id/tv_agent_history"
                        android:textSize="12sp"
                        android:layout_centerHorizontal="true"
                        />
                       <TextView 
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="历史成交(套)"
                        android:layout_marginBottom="5dp"
                        android:layout_below="@+id/tv_agent_history"
                        android:layout_centerHorizontal="true"
                        android:textSize="12sp"
                        />
                </RelativeLayout>
                
                    <RelativeLayout 
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <TextView 
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="30"
                         android:layout_marginTop="5dp"
                        android:layout_centerHorizontal="true"
                        android:id="@+id/tv_agent_pingjunzhouqi"
                        android:textSize="12sp"
                        />
                       <TextView 
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                           android:layout_marginBottom="5dp"
                        android:text="平均成交周期(套)"
                        android:layout_centerHorizontal="true"
                        android:layout_below="@+id/tv_agent_pingjunzhouqi"
                        android:textSize="12sp"
                        />
                </RelativeLayout>
                
                        <RelativeLayout 
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <TextView 
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="30"
                         android:layout_marginTop="5dp"
                        android:layout_centerHorizontal="true"
                        android:id="@+id/tv_agent_sanshiday"
                        android:textSize="12sp"
                        />
                       <TextView 
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:text="近30天带看(套)"
                           android:layout_marginBottom="5dp"
                        android:layout_below="@+id/tv_agent_sanshiday"
                        android:textSize="12sp"
                        />
                </RelativeLayout>
                
                
            </LinearLayout>
            
              <Button 
               android:layout_width="match_parent"
               android:background="#c9c9cb"
               android:layout_height="1dp"
               />
            <TextView 
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:textSize="12sp"
                android:text="本人主要做右安门，陶然居附近的学区房板块，宣府师范第一小学，半步桥小学"
                />
            <Button 
               android:layout_width="match_parent"
               android:background="#c9c9cb"
               android:layout_height="15dp"
               />
     <RelativeLayout 
                 android:layout_width="match_parent"
               android:layout_height="wrap_content"
                >
            <TextView 
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:text="用户评价(63条评论)"
               android:layout_margin="10dp"
               android:id="@+id/tv_agent_yonghupingjia"
               android:textSize="13sp"
               android:layout_alignParentLeft="true"
               android:gravity="center_vertical"
               android:layout_centerHorizontal="true"
               android:textColor="#000000"/>

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_margin="10dp"
                android:src="@drawable/gray_right" />

</RelativeLayout>
            
           <Button 
               android:layout_width="match_parent"
               android:background="#c9c9cb"
               android:layout_height="1dp"
               />
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                
                <com.dumu.housego.util.CircleImageView
                    android:id="@+id/circleImageView2"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:src="@drawable/agent2" />

                <TextView
                    android:id="@+id/tv_agent_usename"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/circleImageView2"
                    android:layout_toRightOf="@+id/circleImageView2"
                    android:layout_marginLeft="5dp"
                    android:text="13787886788" />

                <TextView
                    android:id="@+id/tv_agent_ruzhitime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/tv_agent_usename"
                    android:layout_marginTop="5dp"
                    android:layout_below="@+id/tv_agent_usename"
                    android:textSize="12sp"
                    android:text="入职时间：2~3年" />

                <TextView
                    android:id="@+id/tv_agent_fuwutaidu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/tv_agent_ruzhitime"
                    android:layout_marginTop="5dp"
                    android:layout_below="@+id/tv_agent_ruzhitime"
                    android:text="服务态度"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_agent_biaoqian2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/tv_agent_fuwutaidu"
                    android:layout_marginTop="5dp"
                    android:layout_below="@+id/tv_agent_fuwutaidu"
                    android:text="态度好 细心"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/TextView02"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/tv_agent_usename"
                    android:layout_alignBottom="@+id/tv_agent_usename"
                    android:layout_toRightOf="@+id/tv_agent_usename"
                    android:text="买房人"
                    android:layout_marginLeft="10dp"
                    android:textColor="#ff0000"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/TextView03"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/tv_agent_biaoqian2"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="40dp"
                    android:text="专业技能"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/TextView04"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/tv_agent_biaoqian2"
                    android:layout_alignBottom="@+id/tv_agent_biaoqian2"
                    android:layout_alignLeft="@+id/TextView03"
                    android:text="2016.05.30"
                    android:textSize="12sp" />

            </RelativeLayout>
            
            <Button 
               android:layout_width="match_parent"
               android:background="#c9c9cb"
               android:layout_height="15dp"
               />
            <RelativeLayout 
                 android:layout_width="match_parent"
               android:layout_height="wrap_content"
                >
            <TextView 
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:text="他的成交房源(套)"
               android:layout_margin="10dp"
               android:id="@+id/tv_agent_hishistroy"
               android:textSize="13sp"
               android:layout_alignParentLeft="true"
               android:gravity="center_vertical"
               android:layout_centerHorizontal="true"
               android:textColor="#000000"/>

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_margin="10dp"
                android:src="@drawable/gray_right" />

</RelativeLayout>
            
          <Button 
               android:layout_width="match_parent"
               android:background="#c9c9cb"
               android:layout_height="1dp"
               />  
            
            
            
            
			<!-- 框內內容，scrollView -->
        </LinearLayout>
    </com.dumu.housego.util.MyReboundScrollView>

</RelativeLayout>