<?xml version="1.0" encoding="utf-8"?>
<com.dumu.housego.util.MyReboundScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#F3F3F3"
    tools:ignore="HardcodedText,ContentDescription,RtlSymmetry,RtlHardcoded" >
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
<RelativeLayout 
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/iv_my_back"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:background="@drawable/mybackground" />

    <TextView
        android:id="@+id/tv_login_regist"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/iv_my_back"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="30dp"
        android:text="登录/注册"
        android:textColor="#ffffff"
        android:textSize="16sp" />

   <RelativeLayout
       android:id="@+id/rl_Myershoufang"
       android:layout_width="match_parent"
       android:layout_height="35dp"
       android:background="#ffffff"
       android:layout_alignParentLeft="true"
       android:layout_below="@+id/iv_my_back"
       android:layout_marginTop="18dp" >

       <ImageView
           android:id="@+id/imageView1"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentLeft="true"
           android:layout_centerVertical="true"
           android:layout_marginLeft="20dp"
           android:src="@drawable/ershoufang1" />

       <TextView
           android:id="@+id/textView2"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentLeft="true"
           android:layout_centerVertical="true"
           android:gravity="center_vertical"
           android:paddingLeft="50dp"
           android:text="我的二手房"
           android:textSize="16sp" />

       <ImageView
           android:id="@+id/imageView2"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_centerVertical="true"
           android:layout_marginRight="20dp"
           android:layout_alignParentRight="true"
           android:src="@drawable/gray_right" />

   </RelativeLayout>
   
   <RelativeLayout
       android:id="@+id/rl_Myrenting"
       android:layout_width="match_parent"
       android:layout_height="35dp"
       android:background="#ffffff"
       android:layout_below="@+id/rl_Myershoufang"
       android:layout_alignParentLeft="true"
       android:layout_marginTop="10dp" >

       <ImageView
           android:id="@+id/imageView3"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentLeft="true"
           android:layout_centerVertical="true"
           android:layout_marginLeft="20dp"
           android:src="@drawable/xinfang" />

       <TextView
           android:id="@+id/textView3"
       android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentLeft="true"
           android:layout_centerVertical="true"
           android:gravity="center_vertical"
           android:paddingLeft="50dp"
           android:text="我的出租"
           android:textSize="16sp" />

       <ImageView
           android:id="@+id/imageView4"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_centerVertical="true"
           android:layout_marginRight="20dp"
           android:layout_alignParentRight="true"
           android:src="@drawable/gray_right" />

</RelativeLayout>

   <RelativeLayout
       android:id="@+id/rl_Myentrust"
       android:layout_width="match_parent"
       android:layout_height="35dp"
       android:background="#ffffff"
       android:layout_below="@+id/rl_Myrenting"
       android:layout_marginTop="10dp" >

       <ImageView
           android:id="@+id/imageView5"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentLeft="true"
           android:layout_marginLeft="20dp"
           android:layout_centerVertical="true"
           android:src="@drawable/wodeweituo" />

       <TextView
           android:id="@+id/textView5"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentLeft="true"
           android:layout_centerVertical="true"
           android:gravity="center_vertical"
           android:paddingLeft="50dp"
           android:text="我的委托"
           android:textSize="16sp" />

       <ImageView
           android:id="@+id/imageView6"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentRight="true"
           android:layout_centerHorizontal="true"
           android:layout_marginRight="20dp"
           android:src="@drawable/gray_right" />
   </RelativeLayout>
   
    <RelativeLayout
       android:id="@+id/rl_Yuyueguanli"
       android:layout_width="match_parent"
       android:layout_height="35dp"
       android:background="#ffffff"
       android:layout_below="@+id/rl_Myentrust"
       android:layout_marginTop="10dp" >

       <ImageView
           android:id="@+id/imageView7"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentLeft="true"
           android:layout_centerVertical="true"
           android:layout_marginLeft="20dp"
           android:src="@drawable/yuyueguanli" />

       <TextView
           android:id="@+id/textView7"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentLeft="true"
           android:layout_centerVertical="true"
           android:gravity="center_vertical"
           android:paddingLeft="50dp"
           android:text="预约管理"
           android:textSize="16sp" />

       <ImageView
           android:id="@+id/imageView8"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentRight="true"
           android:layout_centerVertical="true"
           android:layout_marginRight="20dp"
           android:src="@drawable/gray_right" />
   </RelativeLayout>
   
     <RelativeLayout
       android:id="@+id/rl_Histroy"
       android:layout_width="match_parent"
       android:layout_height="35dp"
       android:background="#ffffff"
       android:layout_below="@+id/rl_Yuyueguanli"
       android:layout_marginTop="10dp" >

       <ImageView
           android:id="@+id/imageView9"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_centerInParent="true"
           android:layout_alignParentLeft="true"
           android:layout_marginLeft="20dp"
           android:src="@drawable/lishijilu" />

       <TextView
           android:id="@+id/textView9"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentLeft="true"
           android:layout_centerVertical="true"
           android:gravity="center_vertical"
           android:paddingLeft="50dp"
           android:text="历史记录"
           android:textSize="16sp" />

       <ImageView
           android:id="@+id/imageView10"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentRight="true"
           android:layout_centerInParent="true"
           android:layout_marginRight="20dp"
           android:src="@drawable/gray_right" />
   </RelativeLayout>
     
      <RelativeLayout
          
          android:id="@+id/rl_settings"
          android:layout_width="match_parent"
          android:layout_height="35dp"
          android:layout_below="@+id/rl_Histroy"
          android:layout_marginTop="20dp"
          android:background="#ffffff" >

          <ImageView
              android:id="@+id/imageView11"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignParentLeft="true"
              android:layout_centerVertical="true"
              android:layout_marginLeft="20dp"
              android:src="@drawable/shezhi" />

          <TextView
              android:id="@+id/textView11"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignParentLeft="true"
              android:layout_centerVertical="true"
              android:gravity="center_vertical"
              android:paddingLeft="50dp"
              android:text="设置"
              android:textSize="16sp" />

          <ImageView
              android:id="@+id/imageView12"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignParentRight="true"
              android:layout_centerVertical="true"
              android:layout_marginRight="20dp"
              android:src="@drawable/gray_right" />

          <Button
              android:id="@+id/btn_line"
              android:layout_width="match_parent"
              android:layout_height="1dp"
              android:layout_alignParentLeft="true"
              android:layout_alignParentTop="true"
              android:background="#c9c9cb" />
      </RelativeLayout>

      <com.dumu.housego.util.CircleImageView
          android:id="@+id/iv_my_Photo"
          android:layout_width="80dp"
          android:layout_height="80dp"
          android:layout_above="@+id/tv_login_regist"
          android:layout_alignLeft="@+id/tv_login_regist"
          android:layout_marginBottom="26dp"
          android:src="@drawable/touxiang2" />

      <Button
          android:layout_width="match_parent"
          android:layout_height="10dp"
          android:layout_alignParentLeft="true"
          android:layout_below="@+id/rl_Myershoufang"
          android:background="#ffffff" />

      <Button
          android:layout_width="match_parent"
          android:layout_height="10dp"
          android:layout_alignParentLeft="true"
          android:layout_below="@+id/rl_Myrenting"
          android:background="#ffffff" />

      <Button
          android:layout_width="match_parent"
          android:layout_height="10dp"
          android:layout_alignParentLeft="true"
          android:layout_below="@+id/rl_Myentrust"
          android:background="#ffffff" />

      <Button
          android:layout_width="match_parent"
          android:layout_height="10dp"
          android:layout_alignParentLeft="true"
          android:layout_below="@+id/rl_Yuyueguanli"
          android:background="#ffffff" />
     
  </RelativeLayout>
  </LinearLayout>
  </com.dumu.housego.util.MyReboundScrollView>
