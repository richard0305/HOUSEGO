<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dumu.housego.PuTongMyGuanZhuActivity"
    tools:ignore="ObsoleteLayoutParam,UseCompoundDrawables,RtlSymmetry,HardcodedText,RtlHardcoded,ContentDescription,UselessParent,InefficientWeight,UselessLeaf,TooManyViews,TextFields,LabelFor" >

   
       
        
            <!-- 普通-我的关注 -->

    <RelativeLayout
        android:id="@+id/window_putong_guanzhu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
    android:visibility="gone"
         >
         
        <RelativeLayout
        android:id="@+id/rl_title"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:background="#ee3a43" >

        <TextView
            android:id="@+id/tv_agentpersonal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:paddingLeft="5dp"
            android:text="我的关注"
            android:textColor="#ffffff"
            android:textSize="14sp" />

        <LinearLayout
            android:id="@+id/ll_back_putongguanzhu"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_alignParentLeft="true"
                android:layout_alignTop="@+id/textView1"
                android:src="@drawable/white_left" />

            <TextView
                android:id="@+id/tv_logincancle"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:gravity="center"
                android:paddingLeft="5dp"
                android:text="Back"
                android:textColor="#ffffff"
                android:textSize="14sp" />
        </LinearLayout>

        
    </RelativeLayout>
        
        
        
        
        
        

        <LinearLayout
            android:id="@+id/ll_title"
            android:layout_width="match_parent"
            android:layout_height="50dp"
                 android:layout_below="@+id/rl_title"
            android:orientation="horizontal" >

            <RadioGroup
                android:id="@+id/rg_guanzhu_control"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="#ffffff"
                android:orientation="horizontal" >

                <RadioButton
                    android:id="@+id/guanzhu_ershou"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:background="#ffffff"
                    android:button="@null"
                    android:gravity="center"
                    android:text="二手房"
                    android:textSize="14sp" />

                <RadioButton
                    android:id="@+id/guanzhu_new"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="#ffffff"
                    android:button="@null"
                    android:gravity="center"
                    android:text="新房"
                    android:textSize="14sp" />
            </RadioGroup>
        </LinearLayout>

        <Button
            android:id="@+id/btn_line"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@+id/ll_title"
            android:background="#c9c9cb" />

        <com.dumu.housego.util.NoScrollViewPager
            android:id="@+id/guanzhu_viewpage"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/btn_line" >
        </com.dumu.housego.util.NoScrollViewPager>
    </RelativeLayout>
    <!-- 普通-我的关注 -->


    <!-- fragment -->
    
    <FrameLayout 
        android:visibility="gone"
        android:id="@+id/rl_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        
    </FrameLayout>
    
    
    <!-- fragment -->


    


    <!-- 普通-发布求租 -->

    <RelativeLayout
        android:id="@+id/window_putong_qiuzu"
        android:layout_width="match_parent"
        android:visibility="gone"
        android:layout_height="wrap_content" >

       

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/rl_qiuzu_title"
            android:orientation="vertical" >
            
            <Button
            android:id="@+id/btn_line1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#c9c9cb" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_below="@+id/btn_line"
                android:background="#ffffff" >

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:text="个人称呼:"
                    android:textSize="16sp" />

                <EditText
                    android:id="@+id/et_qiuzu_cehnghu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/textView11"
                    android:layout_marginLeft="30dp"
                    android:layout_toRightOf="@+id/textView11"
                    android:background="@null"
                    android:gravity="center"
                    android:hint="请输入称呼"
                    android:textSize="15sp" />
            </RelativeLayout>
               
            <Button
            android:id="@+id/btn_line2"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="10dp"
            android:background="#c9c9cb" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_below="@+id/btn_line"
                android:background="#ffffff" >

                <TextView
                    android:id="@+id/textView112"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:text="手机号码:"
                    android:textSize="16sp" />

                <EditText
                    android:id="@+id/et_qiuzu_phone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/textView112"
                    android:layout_marginLeft="30dp"
                    android:layout_toRightOf="@+id/textView112"
                    android:background="@null"
                    android:gravity="center"
                    android:hint="请输入号码"
                    android:textSize="15sp" />
            </RelativeLayout>

                <Button
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="10dp"
            android:background="#c9c9cb" />
            <RelativeLayout
                android:id="@+id/rl_qiuzu_fangyuanquyu"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_below="@+id/btn_line"
                android:background="#ffffff" >

                <TextView
                    android:id="@+id/tv_qiuzu_fangyuan"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:text="房源区域:"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/tv_qiuzu_fangyuanquyu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/tv_qiuzu_fangyuan"
                    android:layout_marginLeft="30dp"
                    android:layout_toRightOf="@+id/tv_qiuzu_fangyuan"
                    android:text="请选择"
                    android:textColor="#8A8A8A"
                    android:textSize="15sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/gray_right" />
            </RelativeLayout>
            
                <Button
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="10dp"
            android:background="#c9c9cb" />
            <RelativeLayout
                android:id="@+id/rl_qiuzu_rentingType"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_below="@+id/btn_line"
                android:background="#ffffff" >

                <TextView
                    android:id="@+id/tv_qiuzu_rentingtype1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:text="租赁类型:"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/tv_qiuzu_rentingtype"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/tv_qiuzu_rentingtype1"
                    android:layout_marginLeft="30dp"
                    android:layout_toRightOf="@+id/tv_qiuzu_rentingtype1"
                    android:text="请选择"
                    android:textColor="#8A8A8A"
                    android:textSize="15sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/gray_right" />
            </RelativeLayout>
            
                <Button
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="10dp"
            android:background="#c9c9cb" />
            
            <RelativeLayout
                android:id="@+id/rl_qiuzu_qiwangtingshi"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_below="@+id/btn_line"
                android:background="#ffffff" >

                <TextView
                    android:id="@+id/tv_qiuzu_qiwangtingshi1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:text="期望厅室:"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/tv_qiuzu_qiwangtingshi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/tv_qiuzu_qiwangtingshi1"
                    android:layout_marginLeft="30dp"
                    android:layout_toRightOf="@+id/tv_qiuzu_qiwangtingshi1"
                    android:text="请选择"
                    android:textColor="#8A8A8A"
                    android:textSize="15sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/gray_right" />
            </RelativeLayout>
            
                <Button
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginLeft="10dp"
            android:background="#c9c9cb" />
            <RelativeLayout
                android:id="@+id/rl_qiuzu_qiwangjine"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_below="@+id/btn_line"
                android:background="#ffffff" >

                <TextView
                    android:id="@+id/tv_qiuzu_qiwangjine1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:gravity="center_vertical"
                    android:paddingLeft="10dp"
                    android:text="期望金额:"
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/tv_qiuzu_qiwangjine"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/tv_qiuzu_qiwangjine1"
                    android:layout_marginLeft="30dp"
                    android:layout_toRightOf="@+id/tv_qiuzu_qiwangjine1"
                    android:text="请选择"
                    android:textColor="#8A8A8A"
                    android:textSize="15sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="20dp"
                    android:src="@drawable/gray_right" />
            </RelativeLayout>
            
            
                <Button
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#c9c9cb" />
            
                
                     <Button
                    android:id="@+id/btn_qiuzu_submit"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/btn_submit_shap"
                    android:text="发布"
                    android:textColor="#ffffff"
                    android:textSize="16sp" />
                
                
        </LinearLayout>
        
        
        
    </RelativeLayout>

    <!-- 普通-发布求租 -->

</RelativeLayout>