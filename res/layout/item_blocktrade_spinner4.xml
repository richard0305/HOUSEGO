                        android:background="@drawable/shap_spinner_text"
                        android:gravity="center"
                        android:padding="3dp"
                        android:id="@+id/blockbrade_sx_cunjiti"
                        android:text="村集体用地"
                        android:textColor="#bfbfbf" />

