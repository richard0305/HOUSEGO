                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_weight="1"
                        android:background="@drawable/shap_spinner_text"
                        android:gravity="center"
                        android:padding="3dp"
                        android:text="整体转让"
                        android:textColor="#bfbfbf" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                           android:id="@+id/blockbrade_fangshi_kongguquanzhuanrang"
                        android:layout_weight="1"
                        android:background="@drawable/shap_spinner_text"
                        android:gravity="center"
                        android:padding="3dp"
                        android:text="控股权转让"
                        android:textColor="#bfbfbf" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:background="@drawable/shap_spinner_text"
                        android:text="部分转让"
                        android:gravity="center"
                          android:id="@+id/blockbrade_fangshi_bufenzhuanrang"
                        android:padding="3dp"
                        android:textColor="#bfbfbf" />
                </LinearLayout>
                
                  <LinearLayout
                      android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_weight="1"
                        android:background="@drawable/shap_spinner_text"
                        android:gravity="center"
                        android:padding="3dp"
                        android:id="@+id/blockbrade_fangshi_guquanrongzi"
                        android:text="股权融资"
                        android:textColor="#bfbfbf" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_marginRight="20dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:id="@+id/blockbrade_fangshi_zhaiquanrongzi"
                        android:text="债权融资"
                        android:background="@drawable/shap_spinner_text"
                        android:padding="3dp"
                        android:textColor="#bfbfbf" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="10dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:id="@+id/blockbrade_fangshi_zulinrongzi"
                       	android:text="租赁融资"
                        android:background="@drawable/shap_spinner_text"
                        android:padding="3dp"
                        android:textColor="#bfbfbf" />
                </LinearLayout>             
                           
                           
                           
                           
                <TextView
                    android:id="@+id/blocktrade_spinner_cancle"
                    android:layout_marginTop="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:padding="10dp" 
                    android:textColor="#bfbfbf" 
                    android:text="清空条件"
                    android:layout_gravity="center_horizontal"
                    android:textSize="14sp"
                    />           
                <TextView 
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:background="#fff"
                    />           
                           
                           
                           
                           
            </LinearLayout>
        </com.dumu.housego.util.MyReboundScrollView>
    </LinearLayout>

</RelativeLayout>